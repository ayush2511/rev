#include<stdio.h>
int main() 
{
    int ar[69],n,i, sum = 0;
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);
    printf("Enter the elements;\n");
    for (i = 0; i < n; i++) 
    {
        scanf("%d", &ar[i]);
        sum += ar[i];
    }
    printf("Sum of elements: %d\n", sum);

    return 0;
}
